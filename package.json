{
  "name": "alloy2",
  "version": "0.0.1",
  "main": "src/lib/index.js",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package && npm run tailwind",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "tailwind": "tailwindcss -o package/alloy.css -c tailwind.config.cjs",
    "pack": "npm pack ./package",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@storybook/addon-actions": "^6.5.0-alpha.46",
    "@storybook/addon-essentials": "^6.5.0-alpha.46",
    "@storybook/addon-links": "^6.5.0-alpha.46",
    "@storybook/addon-svelte-csf": "^1.1.0",
    "@storybook/svelte": "^6.5.0-alpha.46",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-storybook": "^0.5.7",
    "eslint-plugin-svelte3": "^3.2.1",
    "postcss": "^8.4.7",
    "postcss-load-config": "^3.1.3",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "rollup-plugin-postcss": "^4.0.2",
    "svelte": "^3.46.4",
    "svelte-check": "^2.2.6",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.10.1",
    "svelte2tsx": "^0.5.5",
    "tailwindcss": "^3.0.23",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
  },
  "type": "module",
  "exports": {
    "./alloy.css": "./alloy.css"
  }
}
